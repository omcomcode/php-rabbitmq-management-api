{
    "name": "andrewmy/rabbitmq-management-api",
    "type": "library",
    "description": "An object oriented wrapper for the RabbitMQ Management HTTP Api; a PHP 8 fork from richardfullmer",
    "homepage": "https://github.com/andrewmy/php-rabbitmq-management-api",
    "keywords": ["rest", "message queue", "rabbitmq"],
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Fullmer",
            "email": "richardfullmer@gmail.com",
            "homepage": "https://github.com/richardfullmer"
        },
        {
            "name": "Andrew M-Y",
            "email": "andrejs@gmail.com",
            "homepage": "https://github.com/andrewmy"
        }
    ],
    "require": {
        "php": "^7.1|^8.0|^8.1",
        "psr/http-message": "^1",
        "php-http/client-implementation": "^1.0",
        "php-http/client-common": "^2",
        "php-http/httplug": "^2",
        "php-http/message-factory": "^1",
        "php-http/discovery": "^1"
    },
    "autoload": {
        "psr-4": {
            "RabbitMq\\ManagementApi\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
